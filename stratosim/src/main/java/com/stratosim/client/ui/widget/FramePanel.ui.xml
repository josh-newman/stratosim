<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">

	<ui:style>
		
	</ui:style>
	
	<ui:image field='closeImage' src="close.png" />

	<g:LayoutPanel ui:field="mainLayout">
		<g:layer left="0px" right="0px" top="0px" height="25px">
			<g:Label ui:field="titleLabel"></g:Label>
		</g:layer>
		<g:layer width="28px" right="0px" top="0px" height="28px">
			<g:PushButton ui:field="closeButton">
				<g:upFace image='{closeImage}' />
			</g:PushButton>
		</g:layer>
		<g:layer left="0px" right="0px" top="30px" bottom="0px">
			<g:SimpleLayoutPanel ui:field="container">
			</g:SimpleLayoutPanel>
		</g:layer>		
	</g:LayoutPanel>

</ui:UiBinder> 